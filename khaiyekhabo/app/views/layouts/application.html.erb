<body id="pageBody">

<div class="container">
<% if alert!=nil %>
    <p class="alert" style="background-color:transparent"><%= alert %></p>
<% end %>
<% if notice!=nil %>
    <p id="notice" style="background-color:transparent"><%= notice %></p>
<%end%>
  <% if flash[:error] %>
    <div id="error" style="color: #b94a48;">
      <%= flash[:error] %>
    </div>
<% end %>
</div>

