<body id="pageBody">
<div class="container">
  <div class="divPanel page-content">

    <div class="row-fluid">

      <div class="span12" id="divMain">


<p id="notice"><%= notice %></p>

<h1>Listing Suggestions</h1>

<table class="table">
  <thead>
    <tr>
      <th>Fullname</th>
      <th>Email</th>
      <th>Subject</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.fullname %></td>
        <td><%= contact.email %></td>
        <td><%= contact.subject %></td>
        <td><%= link_to 'Show More', contact, class: "btn btn-info" %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
</div>
</div>
</div>
</body>

