
<body id="pageBody">
<div class="container">
  <div class="divPanel page-content">

    <div class="row-fluid">

      <div class="span12" id="divMain">

      <p id="notice"><%= notice %></p>

          <h1>Listing Cookcategories</h1>

          <table class="table">
            <thead>
            <tr>
              <th>Name</th>
              <th colspan="3"></th>
            </tr>
            </thead>

            <tbody>
            <% @cookcategories.each do |cookcategory| %>
                <tr>
                  <td><%= cookcategory.name %></td>
                  <td><%= link_to 'Show', cookcategory %></td>
                  <td><%= link_to 'Edit', edit_cookcategory_path(cookcategory) %></td>
                  <td><%= link_to 'Destroy', cookcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
            <% end %>
            </tbody>
          </table>

          <br>

          <%= link_to 'New Cookcategory', new_cookcategory_path %>
        </div>
      </div>
    </div>
  </div>

</body>